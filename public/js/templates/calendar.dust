<div id="header">
	<button id="prev-btn"><</button>
	<h1>{monthName} {year}</h1>
	<button id="next-btn">></button>
</div>
{#prevMonthDays}
	<ol class="prev-month">
		{.}{~n}
	</ol>
{/prevMonthDays}
{#currMonthDays}
	<ol class="curr-month" id="day_{.}">
		{.}{~n}
	</ol>
{/currMonthDays}
{#nextMonthDays}
	<ol class="next-month">
		{.}{~n}
	</ol>
{/nextMonthDays}